generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model User {
  id    String   @id @default(cuid())
  firstName  String
  lastName  String
  quizz Int      @default(0)
  email String  @unique
  password String
}

model MissGrade {
  id    String   @id @default(cuid())
  userId   String
  elegance Int
  beauty   Int
  eloquence Int
  presentation Int
  note       String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
